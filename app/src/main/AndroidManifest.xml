<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xingx.perfectsinger">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Perfect Singer"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AudioProcessing" />
        <activity android:name=".Play" />
        <activity android:name=".Loading" />
        <activity android:name=".Menu" />
        <activity android:name=".RandomRecommend" />
        <activity android:name=".LevelRecommend" />
        <activity android:name=".MyVoice"></activity>
    </application>

</manifest>